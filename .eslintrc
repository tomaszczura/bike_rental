{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "react",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/jsx-one-expression-per-line": 0,
    "comma-dangle": 0,
    "react/no-unused-prop-types": 1,
    "react/require-default-props": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  }
}
